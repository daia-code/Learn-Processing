void setup() {
  size(600, 600);
  background(255);
}
void draw() {

  float[] arr ={100,120,140,160};
  int l=300;
  fill(#ABDAF2);
  for (int i=0; i<arr.length; i=i+1) {
    stroke(#2EA7E5);
    rect(arr[i],arr[i],l,l);
    l=l-40;
  }
  stroke(#FFFFFF);
  line(100,100,400,400);
  line(400,100,100,400);

}
